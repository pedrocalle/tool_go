{
  "family": "tool-go-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::166534940158:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "tool_go",
      "image": "166534940158.dkr.ecr.us-east-2.amazonaws.com/tool-go:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 4000,
          "protocol": "tcp"
        }
      ],
      "environment": [{ "name": "MIX_ENV", "value": "prod" }]
    }
  ]
}
